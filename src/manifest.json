{
  "name": "Upgrade EcoleDirecte",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Upgrade EcoleDirecte"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"],
  "icons": {
    "128": "./upgrade_ecole_directe.png"
  }
}
