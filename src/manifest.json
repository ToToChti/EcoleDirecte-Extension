{
  "name": "Sorion",
  "version": "2.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"]
    }
  ],
  "browser_action": {
    "default_title": "Sorion"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"],
  "icons": {
    "128": "logo.png"
  }
}
